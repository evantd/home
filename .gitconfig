[user]
	name = Evan Dower
	email = edower@indeed.com
[color]
	ui = auto
[core]
	pager = less -FMRiX
	excludesfile = /home/edower/.gitignore
	attributesfile = /Users/edower/indeed/javadev/git/gitattributes
	excludesfile = /home/edower/env/etc/git/gitignore_global
[push]
	default = simple
[alias]
	slog = log --stat --summary -M -C -C
	dag = log --graph --decorate --pretty=oneline --abbrev-commit
	dags = log --graph --decorate --all --stat --summary -M -C -C
	pdiff = diff -M -C -C --patience --color-words -b -w
	rdiff = diff -M -C -C -U999999999 --no-color
	plog = log -p -M -C -C --patience --color-words -b -w
	splog = log --stat --summary -M -C -C -p -M -C -C --patience --color-words -b -w
	backup = push --prune -f edower
	ready = !git push -u origin HEAD:$(git symbolic-ref --short HEAD)
[branch]
	autosetupmerge = always
[diff]
	wordRegex = [[:upper:]][[:lower:]]+|[[:lower:]]+|[[:upper:]]+|[[:digit:]]+|[[:punct:]]|[^[:space:]]
	noprefix = true
[rerere]
	enabled = true
[credential]
	helper = store
[include]
	path = /Users/edower/indeed/javadev/git/merge-drivers
	path = /Users/edower/indeed/javadev/git/aliases
	path = /Users/edower/indeed/javadev/git/general-config
	path = /home/edower/env/etc/git/aliases
	path = /home/edower/env/etc/git/general-config
	path = /home/edower/env/etc/dradis/git/aliases
	path = /home/edower/env/etc/dradis/git/general-config
	path = /home/edower/env/etc/resume/git/aliases
	path = /home/edower/env/etc/resume/git/general-config
[indeed "prefix"]
	feature = jira/
